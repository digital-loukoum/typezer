{
	"name": "typezer",
	"version": "1.0.0",
	"description": "Parse Typescript files and retrieve a synthetic description of defined types and variables",
	"type": "module",
	"main": "library/index.js",
	"scripts": {
		"dev": "esrun --watch test",
		"test": "esrun test",
		"test:inspect": "esrun --inspect test",
		"check": "tsc --noEmit",
		"build": "tsc-esm"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/digital-loukoum/typezer.git"
	},
	"keywords": [
		"typescript",
		"parse",
		"types",
		"definitions"
	],
	"author": "Gin Quin",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/digital-loukoum/typezer/issues"
	},
	"homepage": "https://github.com/digital-loukoum/typezer#readme",
	"dependencies": {
		"@types/node": "^15.6.1",
		"polka": "^0.5.2",
		"typescript": "^4.2.4"
	},
	"devDependencies": {
		"@digitak/esrun": "^1.2.7",
		"@digitak/tsc-esm": "^1.0.3"
	}
}
